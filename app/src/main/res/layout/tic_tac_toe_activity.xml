<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/presenter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text=""
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/presenter">

        <Button
            android:id="@+id/table_cell_00"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="0"
            android:layout_column="0"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />


        <Button
            android:id="@+id/table_cell_01"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="0"
            android:layout_column="1"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />

        <Button
            android:id="@+id/table_cell_02"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="0"
            android:layout_column="2"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />

        <Button
            android:id="@+id/table_cell_10"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="1"
            android:layout_column="0"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />

        <Button
            android:id="@+id/table_cell_11"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="1"
            android:layout_column="1"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />

        <Button
            android:id="@+id/table_cell_12"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="1"
            android:layout_column="2"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />

        <Button
            android:id="@+id/table_cell_20"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="2"
            android:layout_column="0"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />

        <Button
            android:id="@+id/table_cell_21"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="2"
            android:layout_column="1"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />

        <Button
            android:id="@+id/table_cell_22"
            android:layout_width="wrap_content"
            android:layout_height="90dp"
            android:layout_row="2"
            android:layout_column="2"
            android:text=""
            android:textSize="34sp"
            android:onClick="check"
            android:contentDescription="@string/app_name" />
    </GridLayout>



</androidx.constraintlayout.widget.ConstraintLayout>